#
# General Process Model - Harvesting plant material(s)
#

wetlab:hpm a sio:process-model .
wetlab:hpm rdfs:label "Harvesting plant material process" .
wetlab:hpm dc:description "Re-usable, lab-independent minimum definition of harvesting plant materials" .

wetlab:hpm sio:has-agent wetlab:hpmAgent .
wetlab:hpm sio:has-target wetlab:hpmPlant .
wetlab:hpm sio:has-target wetlab:hpmContainer .
wetlab:hpmContainer sio:has-quality sio:empty .
wetlab:hpm sio:has-participants wetlab:hpmTool .
wetlab:hpm sio:has-output wetlab:hpmPlantPart .

#
# General Process Model - Storing material(s)
#

wetlab:store a sio:process-model .
wetlab:store rdfs:label "Storing material" .
wetlab:store dc:description "Re-usable, lab-independent minimum definition of storing materials into a container" .

wetlab:store sio:has-agent wetlab:storeAgent .
wetlab:store sio:has-target wetlab:storeObject .
wetlab:store sio:has-target wetlab:storeContainer .

#
# General Process Model - Incubation
#

wetlab:incubation a sio:process-model .
wetlab:incubation rdfs:label "Incubation" .
wetlab:incubation dc:description "Re-usable, lab-independent minimum definition of incubation" .

